    <refname>&dhpackage;</refname>

    <refpurpose>building and simulating digital circuits</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <cmdsynopsis>
      <command>&dhpackage;</command>

      <arg><option>Qt-options</option></arg>
      <arg><option>KDE-options</option></arg>
      <arg><option><replaceable>FILE</replaceable></option></arg>
    </cmdsynopsis>
